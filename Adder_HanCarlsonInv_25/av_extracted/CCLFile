	 -coupling_cap_threshold_absolute 0.01 \
	 -coupling_cap_threshold_relative 0.001
input_db -type assura \
	 -design_cell_name "Adder_HanCarlsonInv_25 layout Adder" \
	 -directory_name "/local/scratch/btutro/ASSURA_LVS" \
	 -format "DFII" \
	 -run_name "Adder_HanCarlsonInv_25"
